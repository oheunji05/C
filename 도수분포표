#include <stdio.h>
#define NUM 20

int main (){

	int a[20] = { 95,64,78,23,59,72,45,78,65,63,79,50,98,100,24,93,72,87,27,45 };
	int i, range, histo[11];
	for (i = 0; i <= 10; i++) {
		histo[i] = 0;
	}
	for (i = 0; i < NUM; i++) {
		histo[a[i] / 10]++;
	}
	for (i = 0; i <= 10; i++) {
		printf("%3d : %3d\n", i * 10, histo[i]);
	}
	return 0;
}
