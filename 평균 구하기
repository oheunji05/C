#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main(){

	//국어 영어 수학 성적을 정수로 입력받아 평균을 구한 후 
	//평균이 80점 이상이면 '합격!'과 평균 점수를 정수형으로 출력하고,
	//80점 미만이라면 '불합격!'과 80점까지 몇 점이 부족한지 출력 
	
	int kor,eng,math,aver;
	
	printf("입력점수 : ");
	scanf("%d %d %d",&kor,&eng,&math);
	
	aver=(kor+eng+math)/3;
	
	if (aver>=80){
		printf("합격! %d",aver);
	} 
	
	else {
	    printf("불합격! %d",80-aver);
  } 
    
	return 0;
}
