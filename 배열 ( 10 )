#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main (){

	//5개의 양수를 배열에 입력받은 후, 
	//입력받은 모든 수와 최대값을 출력하는 프로그램 
  
	int arr[4],max=-1;
	
	printf("입력 : ");
	for (int i=0;i<5;i++){
		
		scanf ("%d",&arr[i]);
		
		if (arr[i]>max){
			max=arr[i];
		}
		
	}
	 printf("%d %d %d %d %d\n",arr[0],arr[1],arr[2],arr[3],arr[4]);
	 printf("최대값 : %d",max);
   
	return 0;
}
