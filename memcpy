#define _crt_secure_no_warnings
#include <stdio.h>
#include <string.h>

typedef struct _p {
	char name[20];
	int x;
	int y;
}point;

int main() {
	point p1, p2;
	strcpy(p1.name, "구조체 복사");
	p1.x = 10;
	p1.y = 20;
	memcpy(&p2, &p1, sizeof(point));
	printf("%s\n", p2.name);
	printf("%d %d\n", p2.x, p2.y);
  
	return 0;
}
